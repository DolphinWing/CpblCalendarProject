<resources>

    <!-- StepPagerStrip from Roman Nurik's WizardPager -->
    <declare-styleable name="StepPagerStrip">
        <attr name="enableReview" format="boolean" />
    </declare-styleable>

    <!-- ScrollLayout from DateSlider -->
    <declare-styleable name="ScrollLayout">
        <attr name="labelerClass" format="string"/>
        <attr name="labelerFormat" format="string"/>
        <attr name="childWidth" format="dimension"/>
        <attr name="childHeight" format="dimension"/>
    </declare-styleable>
</resources>